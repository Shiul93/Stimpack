classdef stimpack < stimCore
   
    %STIMPACK Summary of this class goes here
    %   Detailed explanation goes here
    
    properties
        
    end
    %% Public methods
    methods
        %% Constructor
        function obj = stimpack(varargin)
			
            
			obj=obj@stimCore(varargin); %superclass constructor
			
			obj.initialiseUI;
			
		end
    end
    %% Hidden methods
    methods (Hidden = true)
        function initialiseUI(obj) 
            stimGUI()
        end
    end
    
end

